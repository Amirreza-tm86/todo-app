{"ast":null,"code":"import 'vue-router';\nimport { useStore } from 'vuex';\nimport router from '../../routes/router';\nimport { getAuth } from '../remove';\nexport default {\n  name: 'navVue',\n  setup() {\n    const store = useStore();\n    const currect = window.location.pathname;\n    const logout = () => {\n      getAuth();\n      store.dispatch('logout');\n      // router.push('/login')\n    };\n\n    const getItem = localStorage.getItem('user');\n    if (currect === '/' && !getItem) {\n      router.push('/signin');\n    }\n    if (currect === '/signin' && getItem) {\n      router.push('/');\n    }\n    if (currect === '/login' && getItem) {\n      router.push('/');\n    }\n    return {\n      logout,\n      store,\n      getItem\n    };\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAO,YAAW;AAClB,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,MAAK,MAAO,qBAAqB;AACxC,SAASC,OAAM,QAAS,WAAW;AACnC,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAIC,MAAM,CAACC,QAAQ,CAACC,QAAO;IACvC,MAAMC,MAAK,GAAI,MAAM;MACjBR,OAAO,EAAC;MACRG,KAAK,CAACM,QAAQ,CAAC,QAAQ;MACvB;IACJ;;IAEA,MAAMC,OAAM,GAAIC,YAAY,CAACD,OAAO,CAAC,MAAM;IAE3C,IAAIN,OAAM,KAAM,GAAE,IAAK,CAACM,OAAO,EAAE;MAC7BX,MAAM,CAACa,IAAI,CAAC,SAAS;IACzB;IAAE,IAAIR,OAAM,KAAM,SAAQ,IAAKM,OAAO,EAAE;MACpCX,MAAM,CAACa,IAAI,CAAC,GAAG;IACnB;IAAE,IAAIR,OAAM,KAAM,QAAO,IAAKM,OAAO,EAAE;MACnCX,MAAM,CAACa,IAAI,CAAC,GAAG;IACnB;IAEA,OAAO;MACHJ,MAAM;MACNL,KAAK;MACLO;IACJ;EACJ;AACJ","names":["useStore","router","getAuth","name","setup","store","currect","window","location","pathname","logout","dispatch","getItem","localStorage","push"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/navVue.vue"],"sourcesContent":["<template>\n    <div class=\"contanier\">\n        <div class=\"topnav\">\n            <router-link class=\"link\" :to=\"getItem ? '/' : '/signin'\">Home</router-link>\n            <router-link id=\"link\" class=\"link\" :to=\"!getItem ? '/signin' : '/'\">