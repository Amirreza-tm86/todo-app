{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    const showinp = false;\n    const updates = null;\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    return {\n      store,\n      showinp,\n      updates\n      // timecreate\n    };\n  },\n\n  created() {\n    // if (localStorage.getItem('token')) {\n    axios.get('/books').then(response => {\n      console.log(response);\n      this.store.state.workouts = response.data;\n    });\n    // }\n  },\n\n  methods: {\n    handlerDelete: function (id) {\n      if (localStorage.getItem('token')) {\n        axios.delete(`/books/${id}`, {\n          headers: {\n            'Authorization': `Bearer ${localStorage.getItem('token')}`\n          }\n        }).then(res => {\n          axios.get('/books', {\n            headers: {\n              'Authorization': `Bearer ${localStorage.getItem('token')}`\n            }\n          }).then(response => {\n            this.store.state.workouts = response.data;\n          });\n        }).catch(err => console.log(err));\n      }\n    },\n    handlerUpdate: function (id) {\n      this.showinp = !this.showinp;\n      if (localStorage.getItem('token')) {\n        if (this.updates !== null) {\n          axios.patch(`/books/${id}`, {\n            Todo: this.updates,\n            headers: {\n              'Authorization': `Bearer ${this.store.state.result}`\n            }\n          }).then(res => {\n            axios.get('/books').then(response => {\n              this.store.state.workouts = response.data;\n            });\n            this.showinp = false;\n            this.updates = null;\n          }).catch(err => console.log(err));\n        }\n      }\n    },\n    showinput: function () {\n      return this.showinp = !this.showinp;\n    },\n    middle: function () {\n      const list = document.getElementById(\"todo\");\n      for (var i = 0; i < list.length; ++i) {\n        list[i].classList.toggle('checked');\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAI,KAAI;IACpB,MAAMC,OAAM,GAAI,IAAG;IACnB;IACA,OAAO;MACHF,KAAK;MACLC,OAAO;MACPC;MACA;IACJ;EACJ,CAAC;;EACDC,OAAO,GAAG;IACN;IACAT,KAAK,CAACU,GAAG,CAAC,QAAQ,EACbC,IAAI,CAACC,QAAO,IAAK;MACdC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;MACrB,IAAI,CAACN,KAAK,CAACS,KAAK,CAACC,QAAO,GAAIJ,QAAQ,CAACP,IAAG;IAC5C,CAAC;IACL;EACJ,CAAC;;EACDY,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzB,IAAIC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;QAE/BrB,KAAK,CAACsB,MAAM,CAAE,UAASH,EAAG,EAAC,EAAE;UACzBI,OAAO,EAAE;YAAE,eAAe,EAAG,UAASH,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;UAAE;QAC1E,CAAC,EACIV,IAAI,CAAEa,GAAG,IAAK;UACXxB,KAAK,CAACU,GAAG,CAAC,QAAQ,EAAE;YAChBa,OAAO,EAAE;cAAE,eAAe,EAAG,UAASH,YAAY,CAACC,OAAO,CAAC,OAAO,CAAE;YAAE;UAC1E,CAAC,EACIV,IAAI,CAACC,QAAO,IAAK;YACd,IAAI,CAACN,KAAK,CAACS,KAAK,CAACC,QAAO,GAAIJ,QAAQ,CAACP,IAAG;UAC5C,CAAC;QACT,CAAC,EACAoB,KAAK,CAAEC,GAAG,IAAKb,OAAO,CAACC,GAAG,CAACY,GAAG,CAAC;MACxC;IACJ,CAAC;IACDC,aAAa,EAAE,UAAUR,EAAE,EAAE;MACzB,IAAI,CAACZ,OAAM,GAAI,CAAC,IAAI,CAACA,OAAM;MAC3B,IAAIa,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,EAAE;QAE/B,IAAI,IAAI,CAACb,OAAM,KAAM,IAAI,EAAE;UACvBR,KAAK,CAAC4B,KAAK,CAAE,UAAST,EAAG,EAAC,EAAE;YACxBU,IAAI,EAAE,IAAI,CAACrB,OAAO;YAClBe,OAAO,EAAE;cAAE,eAAe,EAAG,UAAS,IAAI,CAACjB,KAAK,CAACS,KAAK,CAACe,MAAO;YAAE;UAEpE,CAAC,EACInB,IAAI,CAAEa,GAAG,IAAK;YACXxB,KAAK,CAACU,GAAG,CAAC,QAAQ,EACbC,IAAI,CAACC,QAAO,IAAK;cACd,IAAI,CAACN,KAAK,CAACS,KAAK,CAACC,QAAO,GAAIJ,QAAQ,CAACP,IAAG;YAC5C,CAAC;YACL,IAAI,CAACE,OAAM,GAAI,KAAI;YACnB,IAAI,CAACC,OAAM,GAAI,IAAG;UACtB,CAAC,EACAiB,