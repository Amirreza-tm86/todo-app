{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'workoutForm',\n  data() {\n    const [title, setTitle] = useState('');\n    const [family, setFamily] = useState('');\n    const [number, setNumber] = useState('');\n    const workout = {\n      title,\n      family,\n      number\n    };\n    function onsubmit(e) {\n      console.log(workout);\n      e.preventDefault();\n      axios.post('http://localhost:4000/books', {\n        body: JSON.stringify(workout),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(res => {\n        console.log(res);\n        console.log(number);\n      }).catch(err => console.log(err));\n    }\n    return {\n      onsubmit,\n      title,\n      family,\n      number,\n      setTitle,\n      setFamily,\n      setNumber,\n      workout\n    };\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAI,GAAG;IACH,MAAM,CAACC,KAAK,EAAEC,QAAQ,IAAIC,QAAQ,CAAC,EAAE;IACrC,MAAM,CAACC,MAAM,EAAEC,SAAS,IAAIF,QAAQ,CAAC,EAAE;IACvC,MAAM,CAACG,MAAM,EAAEC,SAAS,IAAIJ,QAAQ,CAAC,EAAE;IACvC,MAAMK,OAAM,GAAI;MAAEP,KAAK;MAAEG,MAAM;MAAEE;IAAO;IACxC,SAASG,QAAQ,CAACC,CAAC,EAAE;MACjBC,OAAO,CAACC,GAAG,CAACJ,OAAO,CAAC;MACpBE,CAAC,CAACG,cAAc,EAAC;MACjBf,KAAK,CAACgB,IAAI,CAAC,6BAA6B,EAAE;QACtCC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACT,OAAO,CAAC;QAC7BU,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACJ,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;QACXT,OAAO,CAACC,GAAG,CAACQ,GAAG,CAAC;QAChBT,OAAO,CAACC,GAAG,CAACN,MAAM,CAAC;MACvB,CAAC,EACAe,KAAK,CAAEC,GAAG,IAAKX,OAAO,CAACC,GAAG,CAACU,GAAG,CAAC;IACxC;IACA,OAAO;MACHb,QAAQ;MACRR,KAAK;MACLG,MAAM;MACNE,MAAM;MACNJ,QAAQ;MACRG,SAAS;MACTE,SAAS;MACTC;IACJ;EACJ;AACJ","names":["axios","name","data","title","setTitle","useState","family","setFamily","number","setNumber","workout","onsubmit","e","console","log","preventDefault","post","body","JSON","stringify","headers","then","res","catch","err"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/workoutForm.vue"],"sourcesContent":["<template>\n    <div>\n        <form :onSubmit=\"onsubmit\">\n            <label>Name:</label>\n            <input type=\"text\" :value=\"title\" @input=\"event => setTitle = event.target.value\">\n            <label>Family:</label>\n            <input type=\"text\" :value=\"family\" @input=\"event => setFamily = event.target.value\">\n            <label>Number:</label>\n            <input type=\"number\" :value=\"number\" @input=\"event => setNumber = event.target.value\">\n            <h1>{{ number }}</h1>\n            <button>Submit</button>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n    name: 'workoutForm',\n    data() {\n        const [title, setTitle] = useState('')\n        const [family, setFamily] = useState('')\n        const [number, setNumber] = useState('')\n        const workout = { title, family, number }\n        function onsubmit(e) {\n            console.log(workout);\n            e.preventDefault()\n            axios.post('http://localhost:4000/books', {\n                body: JSON.stringify(workout),\n                headers: {\n                    'Content-Type': 'application/json'\n                }\n            })\n                .then((res) => {\n                    console.log(res);\n                    console.log(number);\n                })\n                .catch((err) => console.log(err))\n        }\n        return {\n            onsubmit,\n            title,\n            family,\n            number,\n            setTitle,\n            setFamily,\n            setNumber,\n            workout\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}