s: {\n        workoutFormVue\n    },\n    data() {\n        const store = useStore()\n        store.dispatch('getData')\n        const handlerDelete = () => {\n            axios.delete(`http://localhost:4000/books/${store.state.workouts._id}`)\n                .then((res) => {\n                    state.workouts = res.data\n                })\n                .catch((err) => console.log(err))\n        }\n        return {\n            store,\n            handlerDelete\n        }\n    }\n}\n</script>\n\n<style>\nhr {\n    color: slateblue;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}