{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    let update = false;\n    store.dispatch('getData');\n    const handlerDelete = id => {\n      axios.delete(`http://localhost:4000/books/${id}`).then(res => {\n        store.dispatch('getData');\n      }).catch(err => console.log(err));\n    };\n    const handlerUpdate = id => {\n      update = true;\n      axios.patch(`http://localhost:4000/books/${id}`, {\n        Todo: store.state.Todo\n      }).then(res => {\n        store.dispatch('getData');\n      }).catch(err => console.log(err));\n    };\n    return {\n      store,\n      handlerDelete,\n      handlerUpdate,\n      update\n    };\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,IAAIM,MAAK,GAAI,KAAI;IACjBD,KAAK,CAACE,QAAQ,CAAC,SAAS;IACxB,MAAMC,aAAY,GAAKC,EAAE,IAAK;MAC1BV,KAAK,CAACW,MAAM,CAAE,+BAA8BD,EAAG,EAAC,EAC3CE,IAAI,CAAEC,GAAG,IAAK;QACXP,KAAK,CAACE,QAAQ,CAAC,SAAS;MAC5B,CAAC,EACAM,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC;IACA,MAAMG,aAAY,GAAKR,EAAE,IAAK;MAC1BH,MAAK,GAAI,IAAG;MACZP,KAAK,CAACmB,KAAK,CAAE,+BAA8BT,EAAG,EAAC,EAC3C;QACIU,IAAI,EAAEd,KAAK,CAACe,KAAK,CAACD;MAEtB,CAAC,EACAR,IAAI,CAAEC,GAAG,IAAK;QACXP,KAAK,CAACE,QAAQ,CAAC,SAAS;MAC5B,CAAC,EACAM,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC;IACA,OAAO;MACHT,KAAK;MACLG,aAAa;MACbS,aAAa;MACbX;IACJ;EACJ;AACJ","names":["axios","useStore","workoutFormVue","name","components","data","store","update","dispatch","handlerDelete","id","delete","then","res","catch","err","console","log","handlerUpdate","patch","Todo","state"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/navVue.vue"],"sourcesContent":["<template>\n    <div class=\"text-task\">\n        <h3>My Task:</h3>\n    </div>\n    <div class=\"between-flex\" v-for=\"workout in store.state.workouts\">\n        <div>\n            <span>{{ workout.Todo }}</span>\n        </div>\n        <div>\n            <button class=\"btn-del\" @click=\"handlerDelete(workout._id)\">Delete</button>\n            <button class=\"btn-upd\" @click=\"handlerUpdate(workout._id)\">Update</button>\n            <input v-if=\"update\" v-model=\"store.state.Todo\" type=\"text\">\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    data() {\n        const store = useStore()\n        let update = false\n        store.dispatch('getData')\n        const handlerDelete = (id) => {\n            axios.delete(`http://localhost:4000/books/${id}`)\n                .then((res) => {\n                    store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        }\n        const handlerUpdate = (id) => {\n            update = true\n            axios.patch(`http://localhost:4000/books/${id}`,\n                {\n                    Todo: store.state.Todo,\n\n                })\n                .then((res) => {\n                    store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        }\n        return {\n            store,\n            handlerDelete,\n            handlerUpdate,\n            update\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}