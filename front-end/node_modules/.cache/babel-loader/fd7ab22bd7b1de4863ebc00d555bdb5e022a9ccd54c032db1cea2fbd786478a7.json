{"ast":null,"code":"import { ref, provide } from 'vue';\nimport { useStore } from 'vuex';\nimport axios from 'axios';\nexport default {\n  name: 'signin',\n  setup() {\n    const email = ref('');\n    const password = ref('');\n    const store = useStore();\n    provide('getAuth', {\n      getAuth: localStorage.removeItem('user')\n    });\n    const result = ref('');\n    const error = ref(false);\n    const isError = ref(false);\n    const submitSiginup = e => {\n      e.preventDefault();\n      axios.post('http://localhost:4000/signin', {\n        email: store.state.email,\n        password: store.state.password\n      }).then(res => {\n        result.value = res.data;\n        isError.value = false;\n        localStorage.setItem('user', JSON.stringify(res));\n        console.log(res.data.email);\n      }).catch(err => {\n        isError.value = true;\n        error.value = err.response.data.msg;\n        console.log(err.response.data.msg);\n      });\n    };\n    return {\n      email,\n      password,\n      result,\n      error,\n      isError,\n      submitSiginup,\n      store\n    };\n  }\n};","map":{"version":3,"mappings":"AA8BA,SAASA,GAAG,EAAEC,OAAM,QAAS,KAAI;AACjC,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIN,GAAG,CAAC,EAAE;IACpB,MAAMO,QAAO,GAAIP,GAAG,CAAC,EAAE;IACvB,MAAMQ,KAAI,GAAIN,QAAQ,EAAC;IACvBD,OAAO,CAAC,SAAS,EAAE;MACfQ,OAAO,EAAEC,YAAY,CAACC,UAAU,CAAC,MAAM;IAC3C,CAAC;IACD,MAAMC,MAAK,GAAIZ,GAAG,CAAC,EAAE;IACrB,MAAMa,KAAI,GAAIb,GAAG,CAAC,KAAK;IACvB,MAAMc,OAAM,GAAId,GAAG,CAAC,KAAK;IACzB,MAAMe,aAAY,GAAKC,CAAC,IAAK;MACzBA,CAAC,CAACC,cAAc,EAAE;MAClBd,KAAK,CAACe,IAAI,CAAC,8BAA8B,EAAE;QACvCZ,KAAK,EAAEE,KAAK,CAACW,KAAK,CAACb,KAAK;QACxBC,QAAQ,EAAEC,KAAK,CAACW,KAAK,CAACZ;MAC1B,CAAC,EACIa,IAAI,CAAEC,GAAG,IAAK;QACXT,MAAM,CAACU,KAAI,GAAID,GAAG,CAACE,IAAG;QACtBT,OAAO,CAACQ,KAAI,GAAI,KAAI;QACpBZ,YAAY,CAACc,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACL,GAAG,CAAC;QAChDM,OAAO,CAACC,GAAG,CAACP,GAAG,CA