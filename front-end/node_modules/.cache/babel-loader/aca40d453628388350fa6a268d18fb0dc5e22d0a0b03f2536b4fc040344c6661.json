{"ast":null,"code":"import axios from 'axios';\nimport { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'workoutForm',\n  setup() {\n    const title = ref();\n    const Body = ref();\n    const Content = ref();\n    const Code = ref();\n    const store = useStore();\n    return {\n      onsubmit,\n      title,\n      Body,\n      Content,\n      Code\n    };\n  },\n  methods: {\n    onsubmit: function (e) {\n      e.preventDefault();\n      store.state.title = this.title;\n      store.state.Body = this.Body;\n      store.state.Content = this.Body;\n      store.state.Code = this.Code;\n      axios.post('http://localhost:4000/books', {\n        title: store.state.title,\n        Body: store.state.Body,\n        Content: store.state.Content,\n        Code: store.state.Code\n      }).then(res => {\n        console.log(res);\n        // window.location.reload()\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIJ,GAAG,EAAC;IAClB,MAAMK,IAAG,GAAIL,GAAG,EAAC;IACjB,MAAMM,OAAM,GAAIN,GAAG,EAAC;IACpB,MAAMO,IAAG,GAAIP,GAAG,EAAC;IACjB,MAAMQ,KAAI,GAAIP,QAAQ,EAAC;IAEvB,OAAO;MACHQ,QAAQ;MACRL,KAAK;MACLC,IAAI;MACJC,OAAO;MACPC;IACJ;EACJ,CAAC;EACDG,OAAO,EAAE;IACLD,QAAQ,EAAE,UAASE,CAAC,EAAE;MAClBA,CAAC,CAACC,cAAc,EAAC;MACjBJ,KAAK,CAACK,KAAK,CAACT,KAAI,GAAI,IAAI,CAACA,KAAI;MAC7BI,KAAK,CAACK,KAAK,CAACR,IAAG,GAAI,IAAI,CAACA,IAAG;MAC3BG,KAAK,CAACK,KAAK,CAACP,OAAM,GAAI,IAAI,CAACD,IAAG;MAC9BG,KAAK,CAACK,KAAK,CAACN,IAAG,GAAI,IAAI,CAACA,IAAG;MAC3BR,KAAK,CAACe,IAAI,CAAC,6BAA6B,EAAE;QACtCV,KAAK,EAAEI,KAAK,CAACK,KAAK,CAACT,KAAK;QACxBC,IAAI,EAAEG,KAAK,CAACK,KAAK,CAACR,IAAI;QACtBC,OAAO,EAAEE,KAAK,CAACK,KAAK,CAACP,OAAO;QAC5BC,IAAI,EAAEC,KAAK,CAACK,KAAK,CAACN;MACtB,CAAC,EACIQ,IAAI,CAAEC,GAAG,IAAK;QACXC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;QAChB;MACJ,CAAC,EACAG,KAAK,CAAE