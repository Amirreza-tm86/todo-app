xios from 'axios'\nimport { useStore } from 'vuex'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    data() {\n        let workouts = []\n        const store = useStore()\n        store.dispatch('getData')\n        return {\n            workouts,\n            store,\n        }\n    },\n    methods: {\n        handlerDelete: function (id) {\n            // id.preventDefault()\n            axios.delete('http://localhost:4000/books', id)\n                .then((res) => {\n                    this.store.state.workouts = res.data\n                })\n                .catch((err) => console.log(err))\n        }\n    }\n}\n</script>\n\n<style>\nhr {\n    color: slateblue;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}