{"ast":null,"code":"import { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'signin',\n  setup() {\n    const store = useStore();\n    const error = ref(false);\n    const result = ref(false);\n    const isError = ref(false);\n    const submitSiginup = async e => {\n      e.preventDefault();\n      await store.dispatch('useSignup');\n    };\n    return {\n      error,\n      isError,\n      submitSiginup,\n      store,\n      result\n    };\n  }\n};","map":{"version":3,"mappings":"AAgCA,SAASA,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIH,QAAQ,EAAC;IACvB,MAAMI,KAAI,GAAIL,GAAG,CAAC,KAAK;IACvB,MAAMM,MAAK,GAAIN,GAAG,CAAC,KAAK;IACxB,MAAMO,OAAM,GAAIP,GAAG,CAAC,KAAK;IACzB,MAAMQ,aAAY,GAAI,MAAOC,CAAC,IAAK;MAC/BA,CAAC,CAACC,cAAc,EAAC;MACjB,MAAMN,KAAK,CAACO,QAAQ,CAAC,WAAW;IACpC;IAEA,OAAO;MACHN,KAAK;MACLE,OAAO;MACPC,aAAa;MACbJ,KAAK;MACLE;IACJ;EACJ;AACJ","names":["ref","useStore","name","setup","store","error","result","isError","submitSiginup","e","preventDefault","dispatch"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/signin.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit=\"submitSiginup\">\n        <div class=\"container\">\n            <div>\n                <h1>Sign In</h1>\n                <p>Please fill in this form to create an account.</p>\n                <hr>\n\n                <label for=\"email\"><b>Email</b></label>\n                <input class=\"input\" v-model=\"store.state.email\" type=\"text\" placeholder=\"Enter Email\" name=\"email\"\n                    required>\n\n                <label for=\"psw\"><b>Password</b></label>\n                <input class=\"input\" v-model=\"store.state.password\" type=\"password\" placeholder=\"Enter Password\"\n                    name=\"psw\" required>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"signupbtn\">Sign In</button>\n                </div>\n                {{ store.state.email }}  {{ store.state.password }}\n                <div class=\"error\" v-if=\"store.state.isError\">\n                    {{ store.state.error }}\n                </div>\n                <div v-if=\"store.state.result\">\n                    {{ store.state.result }}\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport { useStore } from 'vuex'\nexport default {\n    name: 'signin',\n    setup() {\n        const store = useStore()\n        const error = ref(false)\n        const result = ref(false)\n        const isError = ref(false)\n        const submitSiginup = async (e) => {\n            e.preventDefault()\n            await store.dispatch('useSignup')\n        }\n\n        return {\n            error,\n            isError,\n            submitSiginup,\n            store,\n            result\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}