{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    const store = useStore();\n    var showinp = false;\n    const updates = null;\n    store.dispatch('getData');\n    return {\n      store,\n      showinp,\n      updates\n    };\n  },\n  methods: {\n    handlerDelete: function (id) {\n      axios.delete(`http://localhost:4000/books/${id}`).then(res => {\n        this.store.dispatch('getData');\n      }).catch(err => console.log(err));\n    },\n    handlerUpdate: function (id) {\n      this.showinp = true;\n      axios.patch(`http://localhost:4000/books/${id}`, {\n        Todo: this.updates\n      }).then(res => {\n        this.store.dispatch('getData');\n        this.$refs.input.reset();\n      }).catch(err => console.log(err));\n    }\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,IAAIM,OAAM,GAAI,KAAI;IAClB,MAAMC,OAAM,GAAI,IAAG;IACnBF,KAAK,CAACG,QAAQ,CAAC,SAAS;IACxB,OAAO;MACHH,KAAK;MACLC,OAAO;MACPC;IACJ;EACJ,CAAC;EACDE,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC,EAAE,EAAE;MACzBZ,KAAK,CAACa,MAAM,CAAE,+BAA8BD,EAAG,EAAC,EAC3CE,IAAI,CAAEC,GAAG,IAAK;QACX,IAAI,CAACT,KAAK,CAACG,QAAQ,CAAC,SAAS;MACjC,CAAC,EACAO,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC,CAAC;IACDG,aAAa,EAAE,UAAUR,EAAE,EAAE;MACzB,IAAI,CAACL,OAAM,GAAI,IAAG;MAClBP,KAAK,CAACqB,KAAK,CAAE,+BAA8BT,EAAG,EAAC,EAAE;QAC7CU,IAAI,EAAE,IAAI,CAACd;MACf,CAAC,EACIM,IAAI,CAAEC,GAAG,IAAK;QACX,IAAI,CAACT,KAAK,CAACG,QAAQ,CAAC,SAAS;QAC7B,IAAI,CAACc,KAAK,CAACC,KAAK,CAACC,KAAK,EAAC;MAC3B,CAAC,EACAT,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACxC;EACJ;AACJ","names":["axios","useStore","workoutFormVue","name","components","data","store","showinp","upda