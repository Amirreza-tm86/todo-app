{"ast":null,"code":"import axios from 'axios';\nimport { ref } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'workoutForm',\n  setup() {\n    const title = ref();\n    const Body = ref();\n    const Content = ref();\n    const Code = ref();\n    const store = useStore();\n    console.log(store.state.count);\n    function onsubmit(e) {\n      e.preventDefault();\n      axios.post('http://localhost:4000/books', {\n        title: title.value,\n        Body: Body.value,\n        Content: Content.value,\n        Code: Code.value\n      }).then(res => {\n        console.log(res);\n        window.location.reload();\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n    return {\n      onsubmit,\n      title,\n      Body,\n      Content,\n      Code\n    };\n  }\n};","map":{"version":3,"mappings":"AAiBA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,GAAE,QAAS,KAAI;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIJ,GAAG,EAAC;IAClB,MAAMK,IAAG,GAAIL,GAAG,EAAC;IACjB,MAAMM,OAAM,GAAIN,GAAG,EAAC;IACpB,MAAMO,IAAG,GAAIP,GAAG,EAAC;IACjB,MAAMQ,KAAI,GAAIP,QAAQ,EAAC;IACvBQ,OAAO,CAACC,GAAG,CAACF,KAAK,CAACG,KAAK,CAACC,KAAK,CAAC;IAC9B,SAASC,QAAQ,CAACC,CAAC,EAAE;MACjBA,CAAC,CAACC,cAAc,EAAC;MACjBhB,KAAK,CAACiB,IAAI,CAAC,6BAA6B,EAAE;QACtCZ,KAAK,EAAEA,KAAK,CAACa,KAAK;QAClBZ,IAAI,EAAEA,IAAI,CAACY,KAAK;QAChBX,OAAO,EAAEA,OAAO,CAACW,KAAK;QACtBV,IAAI,EAAEA,IAAI,CAACU;MACf,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;QACXV,OAAO,CAACC,GAAG,CAACS,GAAG,CAAC;QAChBC,MAAM,CAACC,QAAQ,CAACC,MAAM,EAAC;MAC3B,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QACZf,OAAO,CAACC,GAAG,CAACc,GAAG,CAAC;MACpB,CAAC;IACT;IACA,OAAO;MACHX,QAAQ;MACRT,KAAK;MACLC,IAAI;MACJC,OAAO;MACPC;IACJ;EACJ;AACJ","names":["axios","ref","useStore","name","setup","title","Body","Content","Code","store","console","log","state","count","onsubmit","e","preventDefault","post","value","then","res","window","location","reload","catch","err"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Docu