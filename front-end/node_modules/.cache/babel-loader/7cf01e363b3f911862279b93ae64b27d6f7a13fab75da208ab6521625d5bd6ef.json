    })\n                    .catch((err) => console.log(err));\n            }\n        },\n        showinput: function () {\n            return this.showinp = !this.showinp\n        },\n        middle: function () {\n            this.isActive = !this.isActive\n            // if (this.isActive === true) {\n                var elements = document.getElementById(\"middle\")\n                for (var i = 0; i < elements.length; i++) {\n                    this.names += elements[i].name;\n                }\n                this.isActive = !this.isActive\n            // }\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n\n.flex {\n    display: flex;\n}\n\nspan {\n    font-size: 11px;\n    color: black;\n}\n\n.middle {\n    border: 2px solid black;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}