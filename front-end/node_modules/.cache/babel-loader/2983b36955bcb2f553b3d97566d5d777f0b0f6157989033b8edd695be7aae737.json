{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  data() {\n    let workouts = [];\n    const store = useStore();\n    store.dispatch('getData');\n    const handlerDelete = e => {\n      e.preventDefault();\n      axios.delete('http://localhost:4000/books', store.state.workouts._id).then(res => {\n        state.workouts = res.data;\n      }).catch(err => console.log(err));\n    };\n    const preventdefault = e => {\n      e.preventDefault();\n    };\n    return {\n      workouts,\n      store,\n      handlerDelete,\n      preventdefault\n    };\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,IAAIC,QAAO,GAAI,EAAC;IAChB,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvBM,KAAK,CAACC,QAAQ,CAAC,SAAS;IACxB,MAAMC,aAAY,GAAKC,CAAC,IAAK;MACzBA,CAAC,CAACC,cAAc,EAAC;MACjBX,KAAK,CAACY,MAAM,CAAC,6BAA6B,EAAEL,KAAK,CAACM,KAAK,CAACP,QAAQ,CAACQ,GAAG,EACnEC,IAAI,CAAEC,GAAG,IAAK;QACXH,KAAK,CAACP,QAAO,GAAIU,GAAG,CAACX,IAAG;MAC5B,CAAC,EACAY,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACpC;IACA,MAAMG,cAAa,GAAKX,CAAC,IAAK;MAC1BA,CAAC,CAACC,cAAc,EAAC;IACrB;IACA,OAAO;MACHL,QAAQ;MACRC,KAAK;MACLE,aAAa;MACbY;IACJ;EACJ;AACJ","names":["axios","useStore","workoutFormVue","name","components","data","workouts","store","dispatch","handlerDelete","e","preventDefault","delete","state","_id","then","res","catch","err","console","log","preventdefault"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/navVue.vue"],"sourcesContent":["<template>\n    <div v-for=\"workout in store.state.workouts\">\n        <h1>{{ workout.Code }}</h1>\n        <a :href=\"workout._id\">{{ workout.title }}</a>\n        <h1>{{ workout._id }}</h1>\n        <h1>{{ workout.Body }}</h1>\n        <h1>{{ workout.Content }}</h1>\n        <button @click=\"handlerDelete\">Delete</button>\n        <hr>\n    </div>\n    <workoutFormVue />\n</template>\n\n<script>\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    data() {\n        let workouts = []\n        const store = useStore()\n        store.dispatch('getData')\n        const handlerDelete = (e) => {\n            e.preventDefault()\n            axios.delete('http://localhost:4000/books', store.state.workouts._id)\n            .then((res) => {\n                state.workouts = res.data\n            })\n            .catch((err) => console.log(err))\n        }\n        const preventdefault = (e) => {\n            e.preventDefault()\n        }\n        return {\n            workouts,\n            store,\n            handlerDelete,\n            preventdefault\n        }\n    }\n}\n</script>\n\n<style>\nhr {\n    color: slateblue;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}