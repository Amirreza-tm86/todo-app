{"ast":null,"code":"import { ref } from 'vue';\nexport default {\n  name: 'workoutForm',\n  setup() {\n    const title = ref();\n    const family = ref();\n    const number = ref();\n    console.log(title, family, number);\n    const workout = {\n      title,\n      family,\n      number\n    };\n    async function onsubmit(e) {\n      e.preventDefault();\n      const response = await fetch('http://localhost:4000/books', {\n        method: 'POST',\n        body: JSON.stringify(workout),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      });\n      const json = await response.json();\n      if (!response.ok) {\n        console.log('error post methods');\n      }\n      if (response.ok) {\n        console.log('new workout added:', json, workout);\n      }\n    }\n    return {\n      onsubmit,\n      title,\n      family,\n      number,\n      workout\n    };\n  }\n};","map":{"version":3,"mappings":"AAgBA,SAASA,GAAE,QAAS,KAAI;AACxB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIH,GAAG,EAAC;IAClB,MAAMI,MAAK,GAAIJ,GAAG,EAAC;IACnB,MAAMK,MAAK,GAAIL,GAAG,EAAC;IACnBM,OAAO,CAACC,GAAG,CAACJ,KAAK,EAAEC,MAAM,EAAEC,MAAM,CAAC;IAClC,MAAMG,OAAM,GAAI;MAACL,KAAK;MAAEC,MAAM;MAAEC;IAAM;IAEtC,eAAeI,QAAQ,CAACC,CAAC,EAAE;MACvBA,CAAC,CAACC,cAAc,EAAC;MAEjB,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,6BAA6B,EAAE;QACxDC,MAAM,EAAE,MAAM;QACdC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACT,OAAO,CAAC;QAC7BU,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACJ,CAAC;MACD,MAAMC,IAAG,GAAI,MAAMP,QAAQ,CAACO,IAAI,EAAC;MACjC,IAAI,CAACP,QAAQ,CAACQ,EAAE,EAAE;QACdd,OAAO,CAACC,GAAG,CAAC,oBAAoB,CAAC;MACrC;MACA,IAAIK,QAAQ,CAACQ,EAAE,EAAE;QACbd,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEY,IAAI,EAAEX,OAAO;MACnD;IACJ;IACA,OAAO;MACHC,QAAQ;MACRN,KAAK;MACLC,MAAM;MACNC,MAAM;MACNG;IACJ;EACJ;AACJ","names":["ref","name","setup","title","family","number","console","log","workout","onsubmit","e","preventDefault","response","fetch","method","body","JSON","stringify","headers","json","ok"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/workoutForm.vue"],"sourcesContent":["<template>\n    <div>\n        <form :onSubmit=\"onsubmit\">\n            <label>Name:</label>\n            <input type=\"text\" v-model=\"title\">\n            <label>Family:</label>\n            <input type=\"text\" v-model=\"family\">\n            <label>Number:</label>\n            <input type=\"number\" v-model=\"number\">\n            <h1>{{ workout }}</h1>\n            <button>Submit</button>\n        </form>\n    </div>\n</template>\n\n<script>\nimport { ref } from 'vue'\nexport default {\n    name: 'workoutForm',\n    setup() {\n        const title = ref()\n        const family = ref()\n        const number = ref()\n        console.log(title, family, number);\n        const workout = {title, family, number}\n\n        async function onsubmit(e) {\n            e.preventDefault()\n\n            const response = await fetch('http://localhost:4000/books', {\n                method: 'POST',\n                body: JSON.stringify(workout),\n                headers: {\n                    'Content-Type': 'application/json'\n                }\n            })\n            const json = await response.json()\n            if (!response.ok) {\n                console.log('error post methods');\n            }\n            if (response.ok) {\n                console.log('new workout added:', json, workout)\n            }\n        }\n        return {\n            onsubmit,\n            title,\n            family,\n            number,\n            workout\n        }\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}