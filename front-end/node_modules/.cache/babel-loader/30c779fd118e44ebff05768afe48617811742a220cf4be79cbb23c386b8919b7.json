{"ast":null,"code":"import axios from 'axios';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'signin',\n  data() {\n    const store = useStore();\n    return {\n      email: '',\n      password: '',\n      store\n    };\n  },\n  methods: {\n    submitSiginup: async function () {\n      await axios.post('/signup', {\n        email: this.email,\n        password: this.password\n      }).then(response => {\n        console.log(response);\n        this.store.state.email = response.data.email;\n        localStorage.setItem('token', response.data);\n        this.$router.push('/');\n      }).catch(response => {\n        console.log(response.response);\n        this.store.state.error = response.response.data.msg;\n        this.store.state.isError = true;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,IAAI,GAAG;IACH,MAAMC,KAAI,GAAIH,QAAQ,EAAC;IACvB,OAAO;MACHI,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZF;IACJ;EACJ,CAAC;EACDG,OAAO,EAAE;IACLC,aAAa,EAAE,kBAAkB;MAC7B,MAAMR,KAAK,CAACS,IAAI,CAAC,SAAS,EAAE;QACxBJ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACnB,CAAC,EACII,IAAI,CAACC,QAAO,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,IAAI,CAACP,KAAK,CAACU,KAAK,CAACT,KAAI,GAAIM,QAAQ,CAACR,IAAI,CAACE,KAAI;QAC3CU,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEL,QAAQ,CAACR,IAAI;QAC3C,IAAI,CAACc,OAAO,CAACC,IAAI,CAAC,GAAG;MACzB,CAAC,EACAC,KAAK,CAACR,QAAO,IAAK;QACfC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACA,QAAQ,CAAC;QAC9B,IAAI,CAACP,KAAK,CAACU,KAAK,CAACM,KAAI,GAAIT,QAAQ,CAACA,QAAQ,CAACR,IAAI,CAACkB,GAAE;QAClD,IAAI,CAACjB,KAAK,CAACU,KAAK,CAACQ,OAAM,GAAI,IAAG;MAClC,CAAC;IACT;EACJ;AACJ","names":["axios","useStore","name","data","store","email","password","methods","submitSiginup","post","then","response","console","log","state","localStorage","setItem","$router","push","catch","error","msg","isError"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/signin.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit.prevent=\"submitSiginup\">\n        <div class=\"container\">\n            <div>\n                <h1>Sign In</h1>\n                <p>Please fill in this form to create an account.</p>\n                <hr>\n\n                <label for=\"email\"><b>Email</b></label>\n                <input class=\"input\" v-model=\"email\" type=\"text\" placeholder=\"Enter Email\" name=\"email\" required>\n\n                <label for=\"psw\"><b>Password</b></label>\n                <input class=\"input\" v-model=\"password\" type=\"password\" placeholder=\"Enter Password\" name=\"psw\"\n                    required>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"signupbtn\">Sign In</button>\n                </div>\n                <div v-if=\"store.state.isError\" class=\"error\">\n                    {{ store.state.error }}\n                </div>\n                {{ email }} {{ password }}\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nexport default {\n    name: 'signin',\n    data() {\n        const store = useStore()\n        return {\n            email: '',\n            password: '',\n            store\n        }\n    },\n    methods: {\n        submitSiginup: async function () {\n            await axios.post('/signup', {\n                email: this.email,\n                password: this.password\n            })\n                .then(response => {\n                    console.log(response);\n                    this.store.state.email = response.data.email\n                    localStorage.setItem('token', response.data)\n                    this.$router.push('/')\n                })\n                .catch(response => {\n                    console.log(response.response);\n                    this.store.state.error = response.response.data.msg\n                    this.store.state.isError = true\n                })\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}