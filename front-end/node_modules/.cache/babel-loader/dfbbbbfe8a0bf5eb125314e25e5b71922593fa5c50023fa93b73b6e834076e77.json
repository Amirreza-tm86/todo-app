{"ast":null,"code":"import { ref } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'signin',\n  setup() {\n    const email = ref('');\n    const password = ref('');\n    const result = ref('');\n    const isLoading = ref(false);\n    const submitSiginup = e => {\n      e.preventDefault();\n      axios.post('http://localhost:4000/signin', {\n        email: email.value,\n        password: password.value\n      }).then(res => {\n        result.value = res.data;\n        isLoading.value = true;\n      }).catch(err => {\n        isLoading.value = false;\n        console.log(err.message);\n      });\n    };\n    return {\n      email,\n      password,\n      submitSiginup\n    };\n  }\n};","map":{"version":3,"mappings":"AAwBA,SAASA,GAAE,QAAS,KAAI;AACxB,OAAOC,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIJ,GAAG,CAAC,EAAE;IACpB,MAAMK,QAAO,GAAIL,GAAG,CAAC,EAAE;IACvB,MAAMM,MAAK,GAAIN,GAAG,CAAC,EAAE;IACrB,MAAMO,SAAQ,GAAIP,GAAG,CAAC,KAAK;IAC3B,MAAMQ,aAAY,GAAKC,CAAC,IAAK;MACzBA,CAAC,CAACC,cAAc,EAAE;MAClBT,KAAK,CAACU,IAAI,CAAC,8BAA8B,EAAE;QACvCP,KAAK,EAAEA,KAAK,CAACQ,KAAK;QAClBP,QAAQ,EAAEA,QAAQ,CAACO;MACvB,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;QACXR,MAAM,CAACM,KAAI,GAAIE,GAAG,CAACC,IAAG;QACtBR,SAAS,CAACK,KAAI,GAAI,IAAG;MACzB,CAAC,EACAI,KAAK,CAAEC,GAAG,IAAK;QACZV,SAAS,CAACK,KAAI,GAAI,KAAI;QACtBM,OAAO,CAACC,GAAG,CAACF,GAAG,CAACG,OAAO;MAC3B,CAAC;IACT;IACA,OAAO;MACHhB,KAAK;MACLC,QAAQ;MACRG;IACJ;EACJ;AACJ","names":["ref","axios","name","setup","email","password","result","isLoading","submitSiginup","e","preventDefault","post","value","then","res","data","catch","err","console","log","message"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/signin.vue"],"sourcesContent":["<template>\n    <form class=\"formAuth\" @submit=\"submitSiginup\">\n        <div class=\"container\">\n            <div>\n                <h1>Sign In</h1>\n                <p>Please fill in this form to create an account.</p>\n                <hr>\n\n                <label for=\"email\"><b>Email</b></label>\n                <input class=\"input\" v-model=\"email\" type=\"text\" placeholder=\"Enter Email\" name=\"email\" required>\n\n                <label for=\"psw\"><b>Password</b></label>\n                <input class=\"input\" v-model=\"password\" type=\"password\" placeholder=\"Enter Password\" name=\"psw\"\n                    required>\n\n                <div class=\"clearfix\">\n                    <button type=\"submit\" class=\"signupbtn\">Sign In</button>\n                </div>\n            </div>\n        </div>\n    </form>\n</template>\n\n<script>\nimport { ref } from 'vue'\nimport axios from 'axios'\nexport default {\n    name: 'signin',\n    setup() {\n        const email = ref('')\n        const password = ref('')\n        const result = ref('')\n        const isLoading = ref(false)\n        const submitSiginup = (e) => {\n            e.preventDefault();\n            axios.post('http://localhost:4000/signin', {\n                email: email.value,\n                password: password.value\n            })\n                .then((res) => {\n                    result.value = res.data\n                    isLoading.value = true\n                })\n                .catch((err) => {\n                    isLoading.value = false\n                    console.log(err.message)\n                })\n        }\n        return {\n            email,\n            password,\n            submitSiginup\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}