{"ast":null,"code":"// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport { ref } from 'vue';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  setup() {\n    const store = useStore();\n    var showinp = ref(false);\n    const updates = ref(null);\n    store.dispatch('getData');\n    // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n    return {\n      store,\n      showinp,\n      updates\n      // timecreate\n    };\n  },\n\n  methods: {\n    handlerDelete: function (id) {\n      axios.delete(`http://localhost:8000/books/${id}`).then(res => {\n        this.store.dispatch('getData');\n      }).catch(err => console.log(err));\n    },\n    handlerUpdate: function (id) {\n      // this.showinp = true\n      if (this.updates === null) {\n        this.showinp = true;\n      }\n      if (this.updates !== null) {\n        axios.patch(`http://localhost:8000/books/${id}`, {\n          Todo: this.updates\n        }).then(res => {\n          this.store.dispatch('getData');\n          this.showinp = false;\n          this.updates = null;\n        }).catch(err => console.log(err));\n      } else {\n        this.showinp = true;\n      }\n    },\n    middleline: function () {\n      const p = document.getElementById('middle');\n      p.className = \"middle\";\n      if (p.className === \"middle\") {\n        p.className = \"span\";\n      } else {\n        p.className = \"middle\";\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkBA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,GAAE,QAAS,KAAI;AACxB,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIN,QAAQ,EAAC;IACvB,IAAIO,OAAM,GAAIN,GAAG,CAAC,KAAK;IACvB,MAAMO,OAAM,GAAIP,GAAG,CAAC,IAAI;IACxBK,KAAK,CAACG,QAAQ,CAAC,SAAS;IACxB;I