{"ast":null,"code":"import 'vue-router';\nimport { useStore } from 'vuex';\nimport router from '../../routes/router';\nimport { getAuth } from '../remove';\nexport default {\n  name: 'navVue',\n  setup() {\n    const store = useStore();\n    const currect = window.location.pathname;\n    const logout = () => {\n      window.location.reload();\n      getAuth();\n      store.dispatch('logout');\n    };\n    const userwithoutpass = store.state.user.replace('@gmail.com', '');\n    const getItem = localStorage.getItem('user');\n    if (!getItem && currect === '/') {\n      router.push('/signin');\n    }\n    if (getItem && currect === '/signin') {\n      router.push('/');\n    }\n    if (getItem && currect === '/login') {\n      router.push('/');\n    }\n    return {\n      logout,\n      store,\n      getItem,\n      userwithoutpass\n    };\n  }\n};","map":{"version":3,"mappings":"AAcA,OAAO,YAAW;AAClB,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,MAAK,MAAO,qBAAqB;AACxC,SAASC,OAAM,QAAS,WAAW;AACnC,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIL,QAAQ,EAAC;IACvB,MAAMM,OAAM,GAAIC,MAAM,CAACC,QAAQ,CAACC,QAAO;IACvC,MAAMC,MAAK,GAAI,MAAM;MACjBH,MAAM,CAACC,QAAQ,CAACG,MAAM,EAAC;MACvBT,OAAO,EAAC;MACRG,KAAK,CAACO,QAAQ,CAAC,QAAQ;IAC3B;IAEA,MAAMC,eAAc,GAAIR,KAAK,CAACS,KAAK,CAACC,IAAI,CAACC,OAAO,CAAC,YAAY,EAAE,EAAE;IAEjE,MAAMC,OAAM,GAAIC,YAAY,CAACD,OAAO,CAAC,MAAM;IAE3C,IAAI,CAACA,OAAM,IAAKX,OAAM,KAAM,GAAG,EAAE;MAC7BL,MAAM,CAACkB,IAAI,CAAC,SAAS;IACzB;IAAE,IAAIF,OAAM,IAAKX,OAAM,KAAM,SAAS,EAAE;MACpCL,MAAM,CAACkB,IAAI,CAAC,GAAG;IACnB;IAAE,IAAIF,OAAM,IAAKX,OAAM,KAAM,QAAQ,EAAE;MACnCL,MAAM,CAACkB,IAAI,CAAC,GAAG;IACnB;IAEA,OAAO;MACHT,MAAM;MACNL,KAAK;MACLY,OAAO;MACPJ;IACJ;EACJ;AACJ","names":["useStore","router","getAuth","name","setup","store","currect","window","location","pathname","logout","reload","dispatch","userwithoutpass","state","user","replace","getItem","localStorage","push"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/navVue.vue"],"sourcesContent":["<template>\n    <div class=\"contanier\">\n        <div class=\"topnav\">\n            <router-link class=\"link\" :to=\"getItem ? '/' : '/signin'\">Home</router-link>\n            <router-link id=\"link\" class=\"link\" :to=\"!getItem ? '/signin' : '/'\">sign up</router-link>\n            <router-link id=\"link\" class=\"link\" :to=\"!getItem ? '/login' : '/'\">Login</router-link>\n            <button class=\"logout\" v-if=\"store.state.user\" @click=\"logout\">Log out</button>\n            <span v-if=\"store.state.user\">{{ userwithoutpass }}</span>\n        </div>\n        <router-view></router-view>\n    </div>\n</template>\n\n<script>\nimport 'vue-router'\nimport { useStore } from 'vuex';\nimport router from '../../routes/router';\nimport { getAuth } from '../remove';\nexport default {\n    name: 'navVue',\n    setup() {\n        const store = useStore()\n        const currect = window.location.pathname\n        const logout = () => {\n            window.location.reload()\n            getAuth()\n            store.dispatch('logout')\n        }\n\n        const userwithoutpass = store.state.user.replace('@gmail.com', '')\n\n        const getItem = localStorage.getItem('user')\n\n        if (!getItem && currect === '/') {\n            router.push('/signin')\n        } if (getItem && currect === '/signin') {\n            router.push('/')\n        } if (getItem && currect === '/login') {\n            router.push('/')\n        }\n\n        return {\n            logout,\n            store,\n            getItem,\n            userwithoutpass\n        }\n    }\n}\n</script>\n\n<style scoped>\n@import url('../Style/style.css');\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}