{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'workoutForm',\n  data() {\n    const [title, setTitle] = '';\n    const [family, setFamily] = '';\n    const [number, setNumber] = '';\n    function onsubmit(e) {\n      const workout = {\n        title,\n        family,\n        number\n      };\n      console.log(workout);\n      e.preventDefault();\n      axios.post('http://localhost:4000/books', {\n        body: JSON.stringify(workout),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(res => {\n        console.log(res);\n        console.log(number);\n      }).catch(err => console.log(err));\n    }\n    return {\n      onsubmit,\n      title,\n      family,\n      number,\n      setTitle,\n      setFamily,\n      setNumber,\n      workout\n    };\n  }\n};","map":{"version":3,"mappings":"AAgBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAI,GAAG;IACH,MAAM,CAACC,KAAK,EAAEC,QAAQ,IAAI,EAAC;IAC3B,MAAM,CAACC,MAAM,EAAEC,SAAS,IAAI,EAAC;IAC7B,MAAM,CAACC,MAAM,EAAEC,SAAS,IAAI,EAAC;IAE7B,SAASC,QAAQ,CAACC,CAAC,EAAE;MACjB,MAAMC,OAAM,GAAI;QAAER,KAAK;QAAEE,MAAM;QAAEE;MAAO;MACxCK,OAAO,CAACC,GAAG,CAACF,OAAO,CAAC;MACpBD,CAAC,CAACI,cAAc,EAAC;MACjBd,KAAK,CAACe,IAAI,CAAC,6BAA6B,EAAE;QACtCC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACP,OAAO,CAAC;QAC7BQ,OAAO,EAAE;UACL,cAAc,EAAE;QACpB;MACJ,CAAC,EACIC,IAAI,CAAEC,GAAG,IAAK;QACXT,OAAO,CAACC,GAAG,CAACQ,GAAG,CAAC;QAChBT,OAAO,CAACC,GAAG,CAACN,MAAM,CAAC;MACvB,CAAC,EACAe,KAAK,CAAEC,GAAG,IAAKX,OAAO,CAACC,GAAG,CAACU,GAAG,CAAC;IACxC;IACA,OAAO;MACHd,QAAQ;MACRN,KAAK;MACLE,MAAM;MACNE,MAAM;MACNH,QAAQ;MACRE,SAAS;MACTE,SAAS;MACTG;IACJ;EACJ;AACJ","names":["axios","name","data","title","setTitle","family","setFamily","number","setNumber","onsubmit","e","workout","console","log","preventDefault","post","body","JSON","stringify","headers","then","res","catch","err"],"sourceRoot":"","sources":["/home/amirrezatahmasebi/Documents/node-js/front-end/src/components/workoutForm.vue"],"sourcesCon