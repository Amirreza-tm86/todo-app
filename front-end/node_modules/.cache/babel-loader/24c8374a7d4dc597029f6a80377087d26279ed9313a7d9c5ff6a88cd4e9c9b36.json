div>\n        <div>\n            <button class=\"btn-del\" @click=\"handlerDelete(items._id)\">Delete</button>\n            <button class=\"btn-upd\" @click=\"handlerUpdate(items._id), showinput\">Update</button>\n        </div>\n    </div>\n    <input v-if=\"showinp\" ref=\"inp\" v-model=\"updates\" type=\"text\" />\n</template>\n\n<script>\n// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport { ref } from 'vue'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    setup() {\n        const store = useStore()\n        var showinp = ref(false)\n        const updates = ref(null)\n        const names = ref('')\n\n        store.dispatch('getData')\n        const listcheck = () => {\n            var list = document.querySelector('p');\n            list.addEventListener('click', function (ev) {\n                if (ev.target.tagName === 'P') {\n                    ev.target.classList.toggle('checked');\n                }\n            }, false);\n        }\n        // listcheck()\n        // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n        return {\n            store,\n            showinp,\n            updates,\n            names,\n            listcheck\n            // timecreate\n        }\n    },\n    methods: {\n        handlerDelete: function (id) {\n            axios.delete(`http://localhost:8000/books/${id}`)\n                .then((res) => {\n                    this.store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        },\n        handlerUpdate: function (id) {\n            this.showinp = !this.showinp\n            if (this.updates !== null) {\n                axios.patch(`http://localhost:8000/books/${id}`, {\n                    Todo: this.updates\n                })\n                    .then((res) => {\n                        this.store.dispatch('getData')\n                        this.showinp = false\n                        this.updates = null\n                    })\n                    .catch((err) => console.log(err));\n            }\n        },\n        showinput: function () {\n            return this.showinp = !this.showinp\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n\n.flex {\n    display: flex;\n}\n\nspan {\n    font-size: 11px;\n    color: black;\n}\n\np .checked::before {\n    content: '';\n    position: absolute;\n    border-color: #fff;\n    border-style: solid;\n    border-width: 0 2px 2px 0;\n    top: 10px;\n    left: 16px;\n    transform: rotate(45deg);\n    height: 15px;\n    width: 7px;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}