\">\n            <input @click=\"middle\" id=\"myCheck\" type=\"checkbox\">\n            <p class=\"checked\" id=\"todo\">{{ items.Todo }}</p>\n        </div>\n        <div>\n            <button class=\"btn-del\" @click=\"handlerDelete(items._id)\">Delete</button>\n            <button class=\"btn-upd\" @click=\"handlerUpdate(items._id), showinput\">Update</button>\n        </div>\n    </div>\n    <input v-if=\"showinp\" ref=\"inp\" v-model=\"updates\" type=\"text\" />\n</template>\n\n<script>\n// import formatDistanceToNow from 'date-fns/formatDistanceToNow'\nimport axios from 'axios'\nimport { useStore } from 'vuex'\nimport { ref } from 'vue'\nimport workoutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    setup() {\n        const store = useStore()\n        var showinp = ref(false)\n        const updates = ref(null)\n        const names = ref('')\n        const isActive = ref(false)\n        store.dispatch('getData')\n        // let timecreate = ref(formatDistanceToNow(new Date(store.state.date), { addSuffix: true }))\n        return {\n            store,\n            showinp,\n            updates,\n            isActive,\n            names\n            // timecreate\n        }\n    },\n    methods: {\n        handlerDelete: function (id) {\n            axios.delete(`http://localhost:8000/books/${id}`)\n                .then((res) => {\n                    this.store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        },\n        handlerUpdate: function (id) {\n            this.showinp = !this.showinp\n            if (this.updates !== null) {\n                axios.patch(`http://localhost:8000/books/${id}`, {\n                    Todo: this.updates\n                })\n                    .then((res) => {\n                        this.store.dispatch('getData')\n                        this.showinp = false\n                        this.updates = null\n                    })\n                    .catch((err) => console.log(err));\n            }\n        },\n        showinput: function () {\n            return this.showinp = !this.showinp\n        },\n        middle: function () {\n            var checkBox = document.getElementById(\"myCheck\");\n            var todo = document.querySelectorAll(\"p\");\n\n            if (checkBox.checked == true) {\n                todo.className = \"middle\"\n\n            } else {\n                checkBox.style.border = \"2px solid red\";\n            }\n        }\n    }\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n\n.flex {\n    display: flex;\n}\n\nspan {\n    font-size: 11px;\n    color: black;\n}\n\np .checked::before {\n    content: '';\n    position: absolute;\n    border-color: #fff;\n    border-style: solid;\n    border-width: 0 2px 2px 0;\n    top: 10px;\n    left: 16px;\n    transform: rotate(45deg);\n    height: 15px;\n    width: 7px;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}