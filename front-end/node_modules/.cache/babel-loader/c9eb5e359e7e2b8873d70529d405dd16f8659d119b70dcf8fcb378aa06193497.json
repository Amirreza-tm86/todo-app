koutFormVue from './workoutForm.vue'\nexport default {\n    name: 'navVue',\n    components: {\n        workoutFormVue\n    },\n    setup() {\n        const store = useStore()\n        var showinp = ref(false)\n        const updates = ref(null)\n        store.dispatch('getData')\n        const timecreate = formatDistanceToNow(new Date(store.state.date), { addSuffix: true })\n        return {\n            store,\n            showinp,\n            updates,\n            timecreate\n        }\n    },\n    methods: {\n        handlerDelete: function (id) {\n            axios.delete(`http://localhost:5000/books/${id}`)\n                .then((res) => {\n                    this.store.dispatch('getData')\n                })\n                .catch((err) => console.log(err))\n        },\n        handlerUpdate: function (id) {\n            // this.showinp = true\n            if (this.updates === null) {\n                this.showinp = true\n            } if (this.updates !== null) {\n                axios.patch(`http://localhost:5000/books/${id}`, {\n                    Todo: this.updates\n                })\n                    .then((res) => {\n                        this.store.dispatch('getData')\n                        this.showinp = false\n                        this.updates = null\n                    })\n                    .catch((err) => console.log(err));\n            } else {\n                this.showinp = true\n            }\n        }\n    },\n\n}\n</script>\n\n<style>\n@import url('../Style/style.css');\n\n.flex {\n    display: flex;\n}\n\nspan {\n    font-size: 10px;\n    color: #edf2f7;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}