{"ast":null,"code":"import formatDistanceToNow from 'date-fns/formatDistanceToNow';\nimport axios from 'axios';\nimport { useStore } from 'vuex';\nimport { ref, computed } from 'vue';\nimport workoutFormVue from './workoutForm.vue';\nexport default {\n  name: 'navVue',\n  components: {\n    workoutFormVue\n  },\n  setup() {\n    const store = useStore();\n    var showinp = ref(false);\n    const updates = ref(null);\n    store.dispatch('getData');\n    const timecreate = formatDistanceToNow(new Date(store.state.date), {\n      addSuffix: true\n    });\n    return {\n      store,\n      showinp,\n      updates,\n      timecreate\n    };\n  },\n  methods: {\n    handlerDelete: function (id) {\n      axios.delete(`http://localhost:5000/books/${id}`).then(res => {\n        this.store.dispatch('getData');\n      }).catch(err => console.log(err));\n    },\n    handlerUpdate: function (id) {\n      // this.showinp = true\n      if (this.updates === null) {\n        this.showinp = true;\n      }\n      if (this.updates !== null) {\n        axios.patch(`http://localhost:5000/books/${id}`, {\n          Todo: this.updates\n        }).then(res => {\n          this.store.dispatch('getData');\n          this.showinp = false;\n          this.updates = null;\n        }).catch(err => console.log(err));\n      } else {\n        this.showinp = true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,mBAAkB,MAAO,8BAA6B;AAC7D,OAAOC,KAAI,MAAO,OAAM;AACxB,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,OAAOC,cAAa,MAAO,mBAAkB;AAC7C,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,KAAK,GAAG;IACJ,MAAMC,KAAI,GAAIP,QAAQ,EAAC;IACvB,IAAIQ,OAAM,GAAIP,GAAG,CAAC,KAAK;IACvB,MAAMQ,OAAM,GAAIR,GAAG,CAAC,IAAI;IACxBM,KAAK,CAACG,QAAQ,CAAC,SAAS;IACxB,MAAMC,UAAS,GAAIb,mBAAmB,CAAC,IAAIc,IAAI,CAACL,KAAK,CAACM,KAAK,CAACC,IAAI,CAAC,EAAE;MAAEC,SAAS,EAAE;IAAK,CAAC;IACtF,OAAO;MACHR,KAAK;MACLC,OAAO;MACPC,OAAO;MACPE;IACJ;EACJ,CAAC;EACDK,OAAO,EAAE;IACLC,aAAa,EAAE,UAAUC